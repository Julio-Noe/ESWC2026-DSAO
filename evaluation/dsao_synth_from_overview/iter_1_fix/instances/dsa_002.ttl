@prefix dsao: <https://example.org/dsao#> .
@prefix dpv:  <https://w3id.org/dpv#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix :     <https://example.org/dsa/iter0/> .


:case2 a dsao:DataSharingAgreement ;
  dsao:hasProcessing :case2_PA1 ;
  dsao:includesDataset :case2_Dataset1 .

:case2_PA1 a dsao:ProcessingActivity ;
  dpv:hasPurpose <https://w3id.org/dpv#loan_servicing> .
:case2_PA1 dsao:usesData <https://w3id.org/dpv#contact> .
:case2_PA1 dsao:usesData <https://w3id.org/dpv#biometric> .
<https://w3id.org/dpv#biometric> a dpv:SpecialCategoryPersonalData .

:case2_Dataset1 a dsao:SharedDataset ;
  dpv:hasStorage _:s2 .
_:s2 dpv:hasDuration "P12M"^^xsd:duration .
:case2_Dataset1 dsao:disposalMethod "SecureDelete" .


:case2_Transfer1 a dsao:InternationalTransfer ;
  dsao:destinationCountry "NON_DOMESTIC" .
:case2_Transfer1 dsao:transferMechanism "contractual" .
:case2_Transfer1 dsao:hasSafeguard <https://w3id.org/dpv#encryption_at_rest> .
:case2_Transfer1 dsao:hasSafeguard <https://w3id.org/dpv#access_control> .

:case2_RN_proportionality a dsao:ReviewNote ;
  dsao:hintFired "true" ;
  dsao:justifies :case2_PA1 ;
  dsao:guidelineRef "ICO-2021-ยง3.2" ;
  dsao:decision "needs-change" .

:case2_RN_safeguards a dsao:ReviewNote ;
  dsao:justifies :case2_Transfer1 ;
  dsao:guidelineRef "OECD-2025-ยง4" ;
  dsao:decision "pass" .
:case2_PA1 dpv:hasLegalBasis <https://w3id.org/dpv#contract> .
